    "PKief.material-icon-theme": "https://open-vsx.org/api/PKief/material-icon-theme/5.23.0/file/PKief.material-icon-theme-5.23.0.vsix"
  },
  "devDependencies": {
    "@flexbe/eslint-config": "^1.0.11",
    "rimraf": "6.0.1",
    "turbo": "^2.5.4",
    "typescript": "^5.8.3",
    "copy-webpack-plugin": "13.0.0",
    "yargs": "17",
    "@types/yargs": "17"
  }
}
