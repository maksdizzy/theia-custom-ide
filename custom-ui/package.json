{
    "private": true,
    "name": "@flexbe/custom-ui",
    "version": "0.0.0",
    "dependencies": {
        "@theia/core": "1.62.2"
    },
    "devDependencies": {
        "@vitejs/plugin-react": "^4.5.2",
        "rimraf": "^5.0.0",
        "vite": "^6.3.5",
        "vite-plugin-lib-inject-css": "^2.2.2"
    },
    "scripts": {
        "clean": "rm -rf lib node_modules",
        "build": "vite build && npm run dts",
        "watch": "vite build --watch --clearScreen false",
        "dev": "npm run watch",
        "dts": "tsc --emitDeclarationOnly --declaration --outDir lib"
    },
    "theiaExtensions": [
        {
            "frontendPreload": "lib/frontendPreload/index.js",
            "frontend": "lib/frontend/index.js",
            "backend": "lib/backend/index.js"
        }
    ],
    "packageManager": "npm@11.0.0"
}
