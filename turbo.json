{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "clean": {
      "outputs": [],
      "dependsOn": ["^clean"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["lib/**"]
    },
    "start": {
      "dependsOn": [],
      "outputs": []
    },
    "watch": {
      "cache": false,
      "dependsOn": [],
      "outputs": ["lib/**"]
    }
  }
}
